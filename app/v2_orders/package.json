{
    "name": "v2_orders",
    "version": "0.0.1",
    "private": true,
    "sapux": true,
    "description": "v2 Orders",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "start": "fiori run --open test/flpSandbox.html#masterDetail-display",
        "start-local": "fiori run --config ./ui5-local.yaml --open test/flpSandboxMockServer.html#masterDetail-display",
        "build_v2original": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
        "deploy": "fiori add deploy-config",
        "start-mock": "fiori run --open test/flpSandboxMockServer.html#masterDetail-display",
        "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo",
        "build": "npm run clean && ui5 build --include-task=generateManifestBundle generateCachebusterInfo && npm run zip",
        "zip": "cd dist && npx bestzip ../v2orders-content.zip *",
        "clean": "npx rimraf v2orders-content.zip dist"
    },
    "devDependencies": {
        "@ui5/cli": "2.5.0",
        "@ui5/fs": "2.0.1",
        "@ui5/logger": "2.0.0",
        "@sap/ux-ui5-tooling": "1",
        "@sap/ux-specification": "latest",
        "@sap/ui5-builder-webide-extension": "1.0.x",
        "ui5-task-zipper": "^0.3.1"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap/ui5-builder-webide-extension",
            "ui5-task-zipper"
        ]
    }
}
